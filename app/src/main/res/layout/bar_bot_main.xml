<?xml version="1.0" encoding="utf-8"?>
<!-- Main Bottom Bar Template -->
<!-- Used After User is Logged In -->
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bar_bot_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.legacy.MainActivity">

    <!-- Main content -->

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bar_bot_nav_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@color/color_primary"
        app:itemIconTint="@color/color_on_primary"
        app:itemTextColor="@color/color_on_primary"
        app:menu="@menu/menu_main"/>
</FrameLayout>
